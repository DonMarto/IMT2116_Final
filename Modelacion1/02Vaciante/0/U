FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (-0.0693 -0.0717 0);

boundaryField
{
    Canal
    {
        type            noSlip;
    }

    Este
    {
        type            fixedValue;
        value           uniform (-0.0693 -0.0717 0);
    }

    Efluente
    {
        type                   flowRateInletVelocity;
        volumetricFlowRate     constant 0.472;    
        value                  uniform (0 0 0);
    }

    Weste
    {
        type            zeroGradient;
    }

    Ribera_Norte
    {
        type            noSlip;
    }

    Ribera_Sur
    {
        type            noSlip;
    }

    Fondo
    {
        type            noSlip;
    }
    
    Atmosfera
    {
        type            noSlip;
    }
}

// ************************************************************************* //
