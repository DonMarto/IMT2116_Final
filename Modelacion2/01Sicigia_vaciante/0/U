FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    Weste	
    {
        type            fixedValue;
	    value           uniform (0.083 0 0);
    }
    
    Este
    {
        type            zeroGradient;
    }
    
    Efluente
    {
        type                    flowRateInletVelocity;
	    volumetricFlowRate      constant 0.06;
	    value		            uniform (0 0 0);
    }
    
  	Costa
    {
        type            zeroGradient;
    }
    
    Oceano
    {
        type            fixedValue;
	    value	        uniform (0 -0.025 0);
    }
    
    Atmosfera
    {
        type            slip;
    }
    
    Emisario
    {
        type            noSlip;
    }
    
    Fondo
    {
        type            noSlip;
    }
    
    defaultFaces
    {
        type            empty;
    }
}


